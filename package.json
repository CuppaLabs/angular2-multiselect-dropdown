{
  "name": "angular2-multiselect-dropdown",
  "version": "4.6.10",
  "description": "Angular 2 and angular 4 and angular 6 multiselect dropdown component.",
  "keywords": [
    "angular 2 multiselect dropdown",
    "angular 4 multiselect dropdown",
    "angular multiselect",
    "angular dropdown",
    "angular 6 dropdown multiselect",
    "angular 8 dropdown multiselect"
  ],
  "license": "MIT",
  "author": "Pradeep Terli",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-prod": "npm run build angular2-multiselect-dropdown-lib && npm run build-sass && cp \"README.md\" \"dist/angular2-multiselect-dropdown/\" && cp \"LICENSE\" \"dist/angular2-multiselect-dropdown/\" && npm run github-pages",
    "build-sass": "node-sass --include-path scss src/themes/default.theme.scss dist/angular2-multiselect-dropdown/themes/default.theme.css",
    "github-pages": "ng build --prod --aot --base-href /angular2-multiselect-dropdown/",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run angular2-multiselect-dropdown:server:production --bundleDependencies all"
  },
  "dependencies": {
    "@angular/animations": "^8.0.3",
    "@angular/cdk": "^8.1.2",
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/forms": "^8.0.3",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/platform-server": "^8.0.3",
    "@angular/pwa": "^0.803.2",
    "@angular/router": "^8.0.3",
    "@angular/service-worker": "^8.0.3",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@tweenjs/tween.js": "^17.4.0",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.15.8",
    "ngx-highlightjs": "^3.0.3",
    "ngx-virtual-scroller": "^3.0.2",
    "rxjs": "~6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.3",
    "@angular-devkit/build-ng-packagr": "~0.801.3",
    "@angular/cli": "~8.0.4",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/language-service": "^8.0.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.5.2",
    "ng-packagr": "^10.1.1",
    "primeng": "*",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5",
    "webpack-cli": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CuppaLabs/angular2-multiselect-dropdown"
  },
  "bugs": {
    "url": "https://github.com/CuppaLabs/angular2-multiselect-dropdown/issues"
  }
}
