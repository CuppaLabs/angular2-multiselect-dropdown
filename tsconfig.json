{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "angular2-multiselect-dropdown-lib": [
        "dist/angular2-multiselect-dropdown-lib"
      ],
      "angular2-multiselect-dropdown-lib/*": [
        "dist/angular2-multiselect-dropdown-lib/*"
      ]
    }
  }
}